<view class="content"><map class="map" style="{{'height:'+(mapHeight)+';'}}" id="map1" latitude="{{centerLatitude}}" longitude="{{centerLongitude}}" markers="{{markers}}" show-location="{{showLocation}}" scale="{{scale}}" polyline="{{polyline}}" controls="{{controls}}"></map><block wx:if="{{false}}"><view class="orderInfo"><text style="font-size:32rpx;margin-left:30rpx;margin-top:20rpx;">{{orderInfo.station}}</text><text class="textView">{{"出发时间："+orderInfo.time}}</text><text class="textView">{{"当前所在站点："+orderInfo.currentStation}}</text><text class="textView">{{"预计到站："+orderInfo.stationNum}}</text><text class="textView">{{"预计时间："+orderInfo.dowStation}}</text><view class="buttonView"><button data-event-opts="{{[['tap',[['checkDriver',['$event']]]]]}}" class="button" style="background-color:#06B4FD;" bindtap="__e"><text class="butnText">查看司机</text></button><button data-event-opts="{{[['tap',[['contactDriver',['$event']]]]]}}" class="button" style="background-color:#FC4646;" bindtap="__e"><text class="butnText">联系司机</text></button></view></view></block></view>