page {
  background: #F5F5F5;
  padding-bottom: 40rpx;
}
.ob_background {
  width: 100%;
  height: 320rpx;
}
.ob_background image {
  width: 100%;
  height: 100%;
}
.ob_background .Dx_View {
  position: absolute;
  width: 100%;
  top: 180rpx;
  color: #FFFFFF;
  right: 0;
}
.ob_background .Dx_View .Dx_viewAndView {
  position: relative;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_title {
  margin-left: 32rpx;
  font-size: 48rpx;
  font-weight: 300;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_text {
  margin-left: 32rpx;
  font-size: 30rpx;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_image {
  float: right;
  width: 28rpx;
  height: 28rpx;
  margin-right: 32rpx;
  margin-top: 20rpx;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_price {
  float: right;
  font-size: 52rpx;
  margin-right: 6rpx;
  text-align: right;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_priceIcon {
  float: right;
  font-size: 30rpx;
  position: relative;
  margin-right: 2rpx;
  margin-top: 2rpx;
}
.ob_background .Dx_View .Dx_viewAndView .Dx_remarks {
  display: block;
  margin: 20rpx 32rpx;
  font-size: 28rpx;
}
.ob_background .Dx_View {
  top: 64rpx;
}
.ob_background .Dx_View .Dx_image {
  margin-top: 30rpx;
}
.ob_background .Dx_View .Dx_priceIcon {
  margin-top: 8rpx;
}
