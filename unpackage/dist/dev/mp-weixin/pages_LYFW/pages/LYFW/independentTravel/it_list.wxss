.list_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
  -webkit-align-content: flex-start;
          align-content: flex-start;
  font-size: 28rpx;
  margin-top: 32rpx;
}
.list_box .left {
  width: 200rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  border: 1rpx #F5F5F5 double;
}
.list_box .left .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
}
.list_box .left .item .itemView {
  width: 8rpx;
  height: 56rpx;
  margin-top: 16rpx;
}
.list_box .left .item .itemText {
  padding-left: 40rpx;
}
.list_box .left .item:not(:first-child) {
  margin-top: 4px;
}
.list_box .left .item:not(:first-child)::after {
  content: '';
  display: block;
  height: 0;
  border-top: #ddd solid 1px;
  width: 620rpx;
  position: absolute;
  top: -1px;
  right: 0;
  -webkit-transform: scaleY(0.5);
          transform: scaleY(0.5);
  /* 1px像素 */
}
.list_box .left .item.active, .list_box .left .item:active {
  color: #4EB3F7;
}
.list_box .main {
  width: 556rpx;
  border: 1rpx #F5F5F5 double;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
