/* 分类 */
.cate-mask {
  position: fixed;
  left: 0;
  top: 0px;
  bottom: 0;
  width: 100%;
  background: transparent;
  z-index: 95;
  -webkit-transition: .3s;
  transition: .3s;
}
.cate-mask .cate-content {
  width: 630rpx;
  height: 100%;
  background: #fff;
  float: right;
  -webkit-transform: translateX(100%);
          transform: translateX(100%);
  -webkit-transition: .3s;
  transition: .3s;
  z-index: 96;
}
.cate-mask.none {
  display: none;
}
.cate-mask.show {
  background: rgba(0, 0, 0, 0.4);
}
.cate-mask.show .cate-content {
  -webkit-transform: translateX(0);
          transform: translateX(0);
}
.cate .cate-list {
  display: block;
  padding: 40rpx 40rpx;
  border-bottom: 1px #F5F5F5 dotted;
}
.cate .cate-list .cate-Text {
  color: #333333;
  font-size: 34rpx;
}
