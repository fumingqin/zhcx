.groupTour {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 40rpx 32rpx;
  border-bottom: 1px #F5F5F5 dotted;
}
.groupTour .groupContent .contentImage {
  width: 228rpx;
  height: 190rpx;
  border-radius: 8px;
}
.groupTour .groupText {
  margin-left: 25rpx;
}
.groupTour .groupText .contentText {
  font-size: 32rpx;
  font-weight: 40;
  font-family: Source Han Sans SC;
  overflow: hidden;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  text-align: justify;
}
.groupTour .groupText .contentLabel {
  display: block;
  font-size: 28rpx;
  color: #aba9aa;
  margin-top: 21rpx;
}
.groupTour .groupText .groupCost {
  margin-top: 12rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
}
.groupTour .groupText .groupCost .cost {
  font-size: 28rpx;
  color: #FF6600;
}
.groupTour .groupText .groupCost .cost .contentCost {
  font-size: 36rpx;
  color: #FF6600;
}
.groupTour .groupText .groupCost .sellComment {
  position: absolute;
  font-size: 28rpx;
  line-height: 53rpx;
  color: #aba9aa;
  right: 0;
}
.findMore {
  padding: 38rpx 259rpx;
}
.findMore .findMoreText {
  font-size: 30rpx;
  color: #aba9aa;
}

