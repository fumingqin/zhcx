.screenView {
  left: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 97rpx;
  background: #fff;
  z-index: 10;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  background-color: #F5F5F5;
}
.screenView .screenText {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100%;
  font-size: 30rpx;
  color: #888;
  position: relative;
}
.screenView .screenText.current {
  color: #06B4FD;
  background-color: #FFFFFF;
}
.screenView .screenText.current:after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 374rpx;
  height: 0;
  border-bottom: 4rpx solid #06B4FD;
}
