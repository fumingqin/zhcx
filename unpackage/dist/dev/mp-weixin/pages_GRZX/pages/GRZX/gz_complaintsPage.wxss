page {
  background-color: #f6f6f6;
}
.cc_content {
  background-color: #ffffff;
  margin-bottom: 20rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.cc_content .ct_text1 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-weight: bold;
  font-size: 32rpx;
  padding-top: 30rpx;
}
.cc_content .ct_text2 {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 31rpx;
  display: block;
  color: #666;
  border-bottom: 1px #F5F5F5 dotted;
}
.complaintDX {
  background-color: #ffffff;
  margin-bottom: 20rpx;
  padding-top: 30rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.complaintDX .tsdxText {
  font-weight: bold;
  font-size: 32rpx;
}
.complaintDX .tsnrText {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-top: 40rpx;
  font-size: 30rpx;
  color: #333333;
  padding-bottom: 32rpx;
}
.complaintDX .tsnrText .jdticon {
  position: relative;
  top: 9rpx;
  left: 20rpx;
}
.content {
  margin: 0 0 24rpx 0;
  padding-top: 56rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  background-color: #FFFFFF;
}
.content .ComplaintYY {
  border-bottom: 1px #F5F5F5 dotted;
}
.content .ComplaintYY .yyText {
  font-weight: bold;
  font-size: 32rpx;
}
.content .ComplaintYY .yyTitle {
  padding-top: 40rpx;
  font-size: 30rpx;
}
.content .ComplaintYY .num {
  text-align: right;
  font-size: 30rpx;
  color: #AAAAAA;
  padding-bottom: 23rpx;
}
.content .reasonFB {
  position: relative;
  padding-top: 28rpx;
}
.content .reasonFB .fbText {
  font-weight: bold;
  font-size: 32rpx;
}
.content .reasonFB .fbTitle {
  padding-top: 40rpx;
  font-size: 30rpx;
}
.content .reasonFB .tiemBtn {
  position: absolute;
  right: 6rpx;
  font-size: 30rpx;
  text-align: right;
  color: #47A5FC;
}
.cl_bottom {
  width: 100%;
  height: 150rpx;
}
.tjButton {
  position: fixed;
  bottom: 32rpx;
  left: 0;
  right: 0;
  border-radius: 64rpx;
  margin: 0 40rpx;
  background: #1EA2FF;
  text-align: center;
  color: #FFFFFF;
  font-size: 38rpx;
  font-weight: 400;
  box-shadow: 0px 0.2px 0px #aaa;
}

