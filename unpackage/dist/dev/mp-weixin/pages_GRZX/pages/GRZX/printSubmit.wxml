<view><block wx:if="{{print_number==''}}"><view><view class="ps_view"><view class="dl_choice">线路选择</view><view class="lineClass"><view data-event-opts="{{[['tap',[['startStationTap',['$event']]]]]}}" class="start" bindtap="__e">{{routeorigin}}</view><image class="changeImage" src="../../static/GRZX/change.png" mode="aspectFill" data-event-opts="{{[['tap',[['changeClick',['$event']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['endStationTap',['$event']]]]]}}" class="start" style="text-align:right;" bindtap="__e">{{routedestination}}</view></view><view style="border-bottom:1px #F5F5F5 dotted;"></view><view class="dl_choice">站点选择</view><view class="dl_choiceAddress"><text data-event-opts="{{[['tap',[['startStationTap2',['$event']]]]]}}" class="ca_text1" bindtap="__e">{{printSite}}</text></view></view><view class="fb_view2"><view class="fb_Text2">上传图片</view><view class="add-img-box"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="add-img-item"><image class="add-img" src="{{item.path}}" mode="aspectFill" data-event-opts="{{[['tap',[['imgInfo',[index]]]]]}}" bindtap="__e"></image><image class="add-img-del" src="../../static/GRZX/delete.png" data-event-opts="{{[['tap',[['delImg',[index]]]]]}}" bindtap="__e"></image></view></block><block wx:if="{{imgList.length<3}}"><view data-event-opts="{{[['tap',[['openCamera',['$event']]]]]}}" class="add-img-item" bindtap="__e"><image class="add-img" src="../../static/GRZX/add.png"></image></view></block></view></view></view></block><view data-event-opts="{{[['tap',[['submitState',['$event']]]]]}}" class="tjButton" bindtap="__e">提交</view></view>