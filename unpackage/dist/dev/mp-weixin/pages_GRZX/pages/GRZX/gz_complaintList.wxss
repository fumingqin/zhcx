page {
  background-color: #f6f6f6;
}
.screen {
  height: 87rpx;
  background-color: #f6f6f6;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
.screen .screenView {
  left: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 64%;
  height: 87rpx;
  z-index: 10;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  margin-top: 20rpx;
}
.screen .screenView .screenText {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100%;
  font-size: 30rpx;
  color: #888;
  position: relative;
}
.screen .screenView .screenText.current {
  color: #06b4fd;
}
.screen .screenView .screenText.current:after {
  content: '';
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  width: 77rpx;
  height: 0;
  border-bottom: 4rpx solid #06b4fd;
}
