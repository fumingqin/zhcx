page {
  background-color: #F8F8F8;
}
.content {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.boxClass {
  width: 90%;
  margin-top: 20rpx;
  margin-left: 5%;
  background-color: #FFFFFF;
  border-radius: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.flexDirection {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.fontClass {
  color: #2C2D2D;
  font-size: 30rpx;
  padding: 35rpx 2% 35rpx 5%;
  width: 30%;
}
.inputClass {
  width: 70%;
  font-size: 32rpx;
  padding: 35rpx 5% 35rpx 5%;
  text-align: right;
}
.heightClass {
  height: 400rpx;
}
.addClass {
  width: 150rpx;
  height: 150rpx;
  margin-left: 40%;
  margin-top: 90rpx;
}
.textClass {
  font-size: 32rpx;
  margin-top: 25rpx;
  color: #cdcdcd;
  width: 100%;
  text-align: center;
}
.mb {
  margin-bottom: 150rpx;
}
.mt {
  margin-top: 195rpx;
}
.bottomClass {
  background-color: #F8F8F8;
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  height: 120rpx;
}
.btnClass {
  width: 90%;
  position: absolute;
  top: 10rpx;
  left: 5%;
}
.returnClass {
  width: 25rpx;
  height: 40rpx;
  position: absolute;
  top: 100rpx;
  left: 25rpx;
}
.topClass {
  position: fixed;
  top: 0rpx;
  width: 100%;
  height: 170rpx;
  border-bottom: 1rpx solid #F5F5F5;
  background-color: #FFFFFF;
}
.titleClass {
  width: 100%;
  text-align: center;
  font-size: 38rpx;
  /*font-weight: bold; */
  margin-top: 95rpx;
}

