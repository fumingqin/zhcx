page {
  background-color: #F2F2F2;
}
.content {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.bt {
  border-top: 1rpx solid #DBDBDB;
  margin-top: 50rpx;
}
.itemClass {
  width: 100%;
  height: 100rpx;
  background-color: #FFFFFF;
  border-bottom: 1rpx solid #DBDBDB;
  position: relative;
}
.phoneClass1 {
  font-size: 32rpx;
  color: #333333;
  position: absolute;
  left: 5%;
  line-height: 100rpx;
}
.phoneClass2 {
  font-size: 32rpx;
  color: #333333;
  position: absolute;
  right: 5%;
  line-height: 100rpx;
}
.codeClass1 {
  font-size: 32rpx;
  color: #333333;
  position: absolute;
  left: 5%;
  line-height: 100rpx;
}
.codeClass2 {
  font-size: 32rpx;
  color: #333333;
  position: absolute;
  right: 5%;
  top: 25rpx;
  text-align: right;
}
.codeClass3 {
  font-size: 32rpx;
  color: #333333;
  width: 100%;
  text-align: center;
  line-height: 100rpx;
}
.btnClass {
  margin-top: 110rpx;
  width: 90%;
  text-align: center;
  margin-left: 5%;
  color: #FFFFFF;
  padding: 20rpx 0;
  font-size: 32rpx;
  background-color: #ff0000;
  border-radius: 100rpx;
}
.cancellationClass {
  width: 100%;
  font-size: 30rpx;
  text-align: center;
  margin-top: 22rpx;
  color: #7289A8;
}

