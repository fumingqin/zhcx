page {
  background-color: #f6f6f6;
}
.tabsBox {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
  height: 100rpx;
  background-color: #f6f6f6;
}
.pd_view {
  width: 400rpx;
  margin: 40rpx 28rpx;
  margin-bottom: 0rpx;
  border-radius: 32rpx;
  background: #06B4FD;
  text-align: center;
  font-size: 25rpx;
  color: #FFFFFF;
}
.cm_view {
  margin: 0rpx 28rpx;
  margin: 24rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  padding: 20rpx 16rpx;
  padding-bottom: 12rpx;
}
.cm_view .cm_titleView {
  position: relative;
}
.cm_view .cm_titleView .cm_icon {
  position: relative;
  top: 4rpx;
  width: 34rpx;
  height: 38rpx;
}
.cm_view .cm_titleView .cm_title {
  margin-left: 24rpx;
}
.cm_view .cm_titleView .cm_status {
  position: absolute;
  right: 0;
  font-size: 30rpx;
  top: 6rpx;
}
.cm_view .cm_contentView {
  position: relative;
  margin: 14rpx 0;
}
.cm_view .cm_contentView .cm_print {
  width: 200rpx;
  height: 200rpx;
  border-radius: 24rpx;
}
.cm_view .cm_contentView .cm_contentText {
  display: block;
  margin-left: 24rpx;
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #2C2D2D;
}
.cm_view .cm_contentView .cm_contentText2 {
  display: block;
  margin-left: 24rpx;
  font-weight: 400;
  font-size: 32rpx;
  color: #2C2D2D;
}
.cm_view .cm_contentView .cm_contentText3 {
  margin-top: 160rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #2C2D2D;
}
.ci_affirmView {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 995;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 90rpx;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 30rpx;
}
.ci_affirmView .av_atOnceView {
  width: 310rpx;
  height: 80rpx;
  background: #FC4B4B;
  border-radius: 20rpx;
  margin-left: 200rpx;
}
.ci_affirmView .aov_atOnce {
  font-size: 34rpx;
  color: #FFFFFF;
  font-weight: 400;
  padding-top: 20rpx;
  padding-left: 110rpx;
}
.cl_bottom {
  width: 100%;
  height: 150rpx;
}
.tjButton {
  position: fixed;
  bottom: 32rpx;
  left: 0;
  right: 0;
  padding: 24rpx 0;
  border-radius: 64rpx;
  margin: 0 40rpx;
  background: #FC4B4B;
  text-align: center;
  color: #FFFFFF;
  font-size: 38rpx;
  font-weight: 400;
  box-shadow: 0px 0.2px 0px #aaa;
}
.pd_image {
  text-align: center;
  line-height: 50%;
  padding-top: 250rpx;
}
.pd_image .im_image {
  width: 273rpx;
  height: 296rpx;
}
.pd_image .im_text {
  font-size: 30rpx;
  color: #999999;
  line-height: 106rpx;
}

