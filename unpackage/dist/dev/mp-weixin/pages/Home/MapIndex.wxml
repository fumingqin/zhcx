<view class="container"><view class="header bg-ff"><view class="row padding border-b font-26"><view class="col ellipsis-1"><view class="ellipsis-1"><view><text class="color">[当前]</text>{{address.title+''}}</view><view class="color-99 ellipsis-1">{{''+address.address+''}}</view></view></view><view data-event-opts="{{[['tap',[['addressSearch',['$event']]]]]}}" class="padding-l" bindtap="__e"><icon class="icon_search" type="search" size="22" color="#666"></icon></view></view></view><map style="{{'height:'+(mapHeight)+';'}}" id="map" scale="{{map.scale}}" show-location="true" longitude="{{map.longitude}}" latitude="{{map.latitude}}" width="{{map.width}}" height="{{map.height}}" controls="{{map.controls}}" markers="{{map.markers}}" enable-overlooking="{{false}}" enable-satellite="{{false}}" enable-3D="{{false}}" data-event-opts="{{[['regionchange',[['mapChange',['$event']]]]]}}" bindregionchange="__e"><cover-image class="icon-img" src="../../static/Home/icon_position.png"></cover-image></map><view class="footer bg-ff font-26"><scroll-view class="scroll" scroll-y="{{true}}" scroll-top="{{scrollTop}}"><view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['bindAddress',[index]]]]]}}" class="padding border-b position-r" bindtap="__e"><view class="row">{{item.title}}</view><view class="row color-99">{{item.address}}</view><block wx:if="{{checked===index}}"><icon class="icon_circle" type="success" color="#E74246" size="22"></icon></block></view></block></view></scroll-view></view></view>