<view class="content" style="{{'height:'+(imgHeight+'px')+';'}}"><image class="backClass" src="{{background}}"></image><image class="returnClass" src="../../static/GRZX/loginReturn.png" data-event-opts="{{[['tap',[['returnClick',['$event']]]]]}}" bindtap="__e"></image><view class="inputContent"><view class="inputItem phoneNum"><image class="iconClass1" src="../../static/GRZX/shouji.png"></image><input class="inputClass" type="number" placeholder="手机号码" maxlength="11" data-key="phoneNumber" data-event-opts="{{[['input',[['inputChange1',['$event']]]]]}}" bindinput="__e"/></view><view class="inputItem Captcha"><image class="iconClass2" src="../../static/GRZX/yanzhengma.png"></image><input class="inputClass" type="number" placeholder="输入验证码" maxlength="4" data-key="captchaCode" data-event-opts="{{[['input',[['inputChange2',['$event']]]]]}}" bindinput="__e"/></view><view class="getCode style1" id="Code" data-event-opts="{{[['tap',[['getCodeClick',['$event']]]]]}}" bindtap="__e">{{textCode}}</view><text data-event-opts="{{[['tap',[['loginClick',['$event']]]]]}}" class="fontStyle" bindtap="__e">确定</text></view><image class="logoClass" src="{{logo}}"></image></view>