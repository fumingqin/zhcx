page {
  background-color: #f6f6f6;
}
.fr_cardNumber {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-top: 95rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-bottom: 30rpx;
  background-color: #FFFFFF;
  box-shadow: 0px 6px 20px 0px rgba(231, 231, 231, 0.53);
}
.fr_cardNumber .cn_card {
  font-size: 32rpx;
  padding-bottom: 20rpx;
  width: 100%;
  border-bottom: 1px #F5F5F5 dotted;
}
.fr_select {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 0 20rpx;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: #FFFFFF;
  margin-top: 20rpx;
}
.fr_select .sb_button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  border: 1px solid #888;
  border-radius: 12rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 30rpx;
  height: 72rpx;
  width: 212rpx;
  color: #888;
  z-index: 20;
  position: relative;
  margin-top: 37rpx;
}
.fr_select .sb_button:not(:nth-child(3n)) {
  margin-right: 10rpx;
  margin-left: 10rpx;
}
.fr_select .cur {
  z-index: 30;
  color: #FFFFFF;
  border: 1px solid #ff0000;
  background-color: #ff0000;
  border-radius: 12rpx;
}
.fr_select .unCur {
  z-index: 30;
  color: #888;
  border: 1px solid #888;
  background-color: #FFFFFF;
  border-radius: 12rpx;
}
.content {
  margin: 0 0 24rpx 0;
  padding-top: 56rpx;
  padding-left: 32rpx;
  padding-right: 32rpx;
  background-color: #FFFFFF;
}
.content .ComplaintYY {
  border-bottom: 1px #F5F5F5 dotted;
}
.content .ComplaintYY .yyText {
  font-weight: bold;
  font-size: 32rpx;
}
.content .ComplaintYY .yyTitle {
  padding-top: 40rpx;
  font-size: 30rpx;
}
.content .ComplaintYY .num {
  text-align: right;
  font-size: 30rpx;
  color: #AAAAAA;
  padding-bottom: 23rpx;
}
.cl_bottom {
  width: 100%;
  height: 150rpx;
}
.tjButton {
  position: fixed;
  bottom: 32rpx;
  left: 0;
  right: 0;
  padding: 24rpx 0;
  border-radius: 64rpx;
  margin: 0 40rpx;
  background: #1EA2FF;
  text-align: center;
  color: #FFFFFF;
  font-size: 38rpx;
  font-weight: 400;
  box-shadow: 0px 0.2px 0px #aaa;
}

