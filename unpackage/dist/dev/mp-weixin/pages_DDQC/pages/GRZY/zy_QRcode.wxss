page {
  background: -webkit-linear-gradient(top, #9cffbb, #35c762);
}
.qc_top {
  background: #35c762;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  width: 684rpx;
  height: 100%;
  margin-top: 182rpx;
  margin-left: 30rpx;
  text-align: center;
  padding: 19rpx 0;
  box-shadow: 0px 6px 20px 0px rgba(53, 199, 98, 0.53);
}
.qc_top .to_text {
  font-size: 36rpx;
  color: #FFFFFF;
}
.qc_bottom {
  width: 684rpx;
  height: 626rpx;
  background-color: #FFFFFF;
  margin-left: 30rpx;
  box-shadow: 0px 6px 20px 0px rgba(53, 199, 98, 0.53);
  border-bottom-left-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
}
.qc_bottom .bt_view {
  padding-top: 20rpx;
  text-align: center;
  padding-bottom: 20rpx;
}
.qc_bottom .bt_view .vi_text {
  color: #666666;
  font-size: 28rpx;
}
.qc_bottom .bt_view .vi_text2 {
  color: #1ea2ff;
  font-size: 28rpx;
}
.complaintDX {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  background-color: #ffffff;
  margin-bottom: 20rpx;
  padding-top: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  text-align: center;
  border-top: 1px #F5F5F5 dotted;
}
.complaintDX .tsdxText {
  font-weight: bold;
  font-size: 32rpx;
}
.complaintDX .tsnrText {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: absolute;
  font-size: 32rpx;
  right: 0;
  color: #333333;
  padding-bottom: 32rpx;
  padding-left: 20rpx;
}
.complaintDX .tsnrText .jdticon {
  position: relative;
}

